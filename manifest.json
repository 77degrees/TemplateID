{
  "manifest_version": 3,
  "name": "Legal Hold Template ID Extractor",
  "description": "Extracts Legal Hold Template IDs on the Exterro Legal Hold Templates settings page.",
  "version": "1.1.1",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://*.exterro.net/fusion/matter/index.html*",
        "https://*.exterro.com/fusion/matter/index.html*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
